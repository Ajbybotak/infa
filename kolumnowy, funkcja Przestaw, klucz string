string Przestaw(string tj, string kl)
{
    string szyfrogram = "";
    int i, j;
    while (tj.size() % kl.size() != 0) tj = tj + 'X';
    for (j = 0; j < kl.size(); j++)
        for (i = 0; i < tj.size() / kl.size(); i++)
            szyfrogram += tj[i * kl.size() + kl[j] - '0'];
    return szyfrogram;
}
