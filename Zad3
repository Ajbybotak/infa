#include <iostream>
#include <algorithm>
#include <string>
using namespace std;

int main() {
    string s1, s2;

    cout << "Podaj pierwsze slowo ";
    cin >> s1;
    cout << "Podaj drugie slowo ";
    cin >> s2;

    for (int i = 0; i < s1.length(); i++) s1[i] = toupper(s1[i]);
    for (int i = 0; i < s2.length(); i++) s2[i] = toupper(s2[i]);

    sort(s1.begin(), s1.end());
    sort(s2.begin(), s2.end());

    if (s1 == s2)
        cout << "To sa anagramy." << endl;
    else
        cout << "To NIE sa anagramy." << endl;

    return 0;
}
